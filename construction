import torch

def create_line(input_shape, output_shape):
    return torch.nn.Linear(input_shape, output_shape)

def set_serial(left, side):
    return torch.nn.Sequential(left, side)

def set_parallel(left, right):
    return torch.nn.ModuleList(left, right)

def load_state(model, checkpoint):
    return model.load(checkpoint)

def find_root(model, epsilon, tau, delta):
    pass
